urxvt*background: black
urxvt*foreground: white
!!URxvt.font: xft:Bitstream Vera Sans Mono:pixelsize=12
URxvt.scrollBar: false
!urxvt.font: xft:Meslo LG S DZ:regular:pixelsize=15

URxvt.font:xft:Menlo:style=Regular:pixelsize=14
URxvt.boldFont:xft:Menlo:style=Regular:pixelsize=14
URxvt.italicFont:xft:Menlo:style=Regular:pixelsize=14
URxvt.boldItalicFont:xft:Menlo:style=Regular:pixelsize=14
!urxvt*font: TTF:Hack:pixelsize=10
!urxvt*faceSize: 14 



xterm*background: black
xterm*foreground: white
xterm*faceName: Hack
xterm*faceSize: 10 




! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
! Use extended color scheme
rofi.color-enabled:                  true
! Color scheme for normal row
rofi.color-normal:                   argb:00000000, #00b0ef, argb:00000000, #00b0ef,#0060a0
! Color scheme for urgent row
rofi.color-urgent:                   argb:00000000, #ffa0a0, argb:00000000, #ffa0a0,#0060a0
! Color scheme for active row
rofi.color-active:                   argb:00000000, #a0ffa0, argb:00000000, #a0ffa0,#0060a0
! Color scheme window
rofi.color-window:                   argb:ee0060a0, #00b0ef,#00b0ef
